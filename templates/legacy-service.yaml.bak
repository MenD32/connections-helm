# This file has been replaced by frontend-service.yaml and backend-service.yaml
# Kept for reference

apiVersion: v1
kind: Service
metadata:
  name: {{ include "connections-helm.fullname" . }}
  labels:
    {{- include "connections-helm.labels" . | nindent 4 }}
spec:
  type: {{ .Values.service.type }}
  ports:
    - port: {{ .Values.service.port }}
      targetPort: http
      protocol: TCP
      name: http
  selector:
    {{- include "connections-helm.selectorLabels" . | nindent 4 }}
